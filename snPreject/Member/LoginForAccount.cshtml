<script src="/JS/VCode.js?v=1"></script>
<script type="text/javascript">
    //====登录开始

    $(document).ready(function () {
        var mobile = GetCookie("m");
        var password = GetCookie("p");
        if (mobile != "" && password != "") {
            $("#login_chkSaveAccount").prop("checked", true);
            $("#txtLoginMobile").val(mobile);
            $("#txtLoginPassword").val(password);
        }
    });

    //切换效果
    $(".sn_login ul li").click(function (event) {
        event.preventDefault();
        $(".sn_login ul li a").removeClass("hover");
        $(".changeLogin").removeClass("showCurrent");
        $(this).children("a").addClass("hover");

        var loginIndex = $(this).index();
        if (loginIndex == 0) {
            $.ajax({
                url: "/Member/Login",
                type: "get",
                ShowLoading: false,
                dataType: "text",
                success: function (res) {
                    $("#login").html(res);
                }
            });
        }

        $(".changeLogin").eq(loginIndex).addClass("showCurrent");
    });

    $("#txtLoginPassword").on("keypress", function () {
        if (event.keyCode == 13) {
            AJAX_Login_Save();
        }
    });

    //登录
    function AJAX_Login_Save() {
        var mobile = $.trim($("#txtLoginMobile").val());
        var password = $.trim($("#txtLoginPassword").val());

        if (mobile == '') {
            $("#login_mobileVerify").html("&#xe614;").addClass("inputNoIcon").css("display", "block");
            $("#login_errorInfo").text("请输入手机号码！");
            return false;
        }

        if (mobile.search(/^[1][3,4,5,6,7,8,9][0-9]{9}$/g)) {
            $("#login_mobileVerify").html("&#xe614;").addClass("inputNoIcon").css("display", "block");
            $("#login_errorInfo").text("请输入正确的手机号码！");
            return false;
        }

        $("#login_mobileVerify").html("&#xe695;").addClass("inputYesIcon").css("display", "block");
        $("#login_errorInfo").text("");

        if (password == '') {
            $("#login_pwdVerify").html("&#xe614;").addClass("inputNoIcon").css("display", "block");
            $("#login_errorInfo").text("请输入密码！");
            return false;
        }

        if (password.search(/^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,}$/g)) {
            $("#login_pwdVerify").html("&#xe614;").addClass("inputNoIcon").css("display", "block");
            $("#login_errorInfo").text("请设置6-20位数字字母组合的密码！");
            return false;
        }

        $("#login_pwdVerify").html("&#xe695;").addClass("inputYesIcon").css("display", "block");
        $("#login_errorInfo").text("");

        $.ajax({
            url: "/Member/LoginForAccount/Save",
            type: "post",
            data: $("#form_Login").serializeArray(),
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    if ($("#login_chkSaveAccount").is(":checked")) {
                        SetCookie("m", mobile, 30);
                        SetCookie("p", password, 30);
                    }
                    else {
                        SetCookie("m", null, -1);
                        SetCookie("p", null, -1);
                    }

                    location.href = "/Member/";
                }
                else {
                    $("#login_errorInfo").text(res.ErrorInfo);
                }
            }
        });
    }

    //忘记密码
    function UpdatePwd() {
        $("#login_Tab").hide();
        $("#loginMobile").hide();
        $("#updatePwd").show();
        $("#updatePwd1").show();
        $("#updatePwd3").hide();
        clearInterval(loginTimer);
        GetUpdatePwdQrCode();
    }

    //绑定手机
    function BindMobile() {
        $("#login_Tab").hide();
        $("#loginMobile").hide();
        $("#bindMobile").show();
        $("#bindMobile1").show();
        $("#bindMobile3").hide();
        clearInterval(loginTimer);
        GetBindMobileQrCode();
        InitVCode("imgVerifyCode");
    }

    function GetCookie(name) {
        if (document.cookie.length < 1) return "";

        var reg = RegExp(name + '=([^;]+)');
        var arr = document.cookie.match(reg);
        if (arr) {
            return arr[1];
        }
        else {
            return "";
        }
    }

    function SetCookie(name, value, day) {
        var date = new Date();
        date.setDate(date.getDate() + Number(day));
        document.cookie = name + "=" + value + ";expires=" + date;
    }

    //====登录结束


    //====立即绑定手机开始

    var bindMobileTimer, bindMobileTicket;

    //刷新二维码
    function RefreshBindMobilenQrCode() {
        GetBindMobileQrCode();
    }

    //获取二维码
    function GetBindMobileQrCode() {
        $.ajax({
            url: "/Member/LoginForAccount/GetQRCode_WXBind",
            type: "get",
            ShowLoading: false,
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    if (res.Ticket == null) {
                        $("#bindMobile_qrcodeFailed").show();
                    }
                    else {
                        $("#bindMobile_loading").hide();
                        $("#bindMobile_qrcodeWX").show().attr("src", res.QrCode);
                        bindMobileTicket = res.Ticket;
                        $("#bindMobile_qrcodeFailed").hide();
                        IntervalBindMobileQrStatus();
                    }
                }
            }
        });
    }

    //检查二维码状态
    function IntervalBindMobileQrStatus() {
        var num = 30;
        bindMobileTimer = setInterval(function CheckBindMobileQrStatus() {
            num--;

            $.ajax({
                url: "/Member/LoginForAccount/CheckQrStatus_WXBind?ticket=" + bindMobileTicket,
                type: "get",
                ShowLoading: false,
                dataType: "json",
                success: function (res) {
                    if (res.StatusCode == "Success") {
                        if (res.IsDisable == true) {
                            clearInterval(bindMobileTimer);
                            $("#bindMobile_qrcodeInfo").text("您的账号已被禁用！");
                            return;
                        }

                        if (res.QrStatus == true) {
                            clearInterval(bindMobileTimer);
                            clearTimeout(SMSVCodeNumber_timer);
                            $("#bindMobile1").hide();
                            $("#bindMobile2").show();
                            $(".resettop dl").addClass("current_tab02");
                            $("#hddBindMobileOpenID").val(res.OD);
                            return;
                        }
                    }
                }
            });

            if (num == 0) {
                clearInterval(bindMobileTimer);
                layer.closeAll();
            }

        }, 1000);
    }

    //获取短信验证码
    function GetBindMobileSMSVCode() {
        var mobile = $.trim($("#txtBindMobile").val());

        if (mobile == '') {
            $("#bindMobile_Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入手机号码！");
            return false;
        }

        if (mobile.search(/^[1][3,4,5,6,7,8,9][0-9]{9}$/g)) {
            $("#bindMobile_Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入正确的手机号码！");
            return false;
        }

        $("#bindMobile_Verify").text("√");
        $("#bindMobile_errorInfo").text("");

        if ($.trim($("#txtBindMobileVerifyCode").val()) == '') {
            $("#bindMobile_CodeVerify").text("×");
            $("#bindMobile_errorInfo").text("请输入图形验证码！");
            return false;
        }

        $("#bindMobile_CodeVerify").text("√");
        $("#bindMobile_errorInfo").text("");

        $.ajax({
            url: "/Member/LoginForAccount/SendSMSVCode_BindMobile",
            type: "post",
            data: $("#form_BindMobile").serializeArray(),
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    $("#bindMobile_errorInfo").html("短信验证码已发送到您的手机！");
                    btnSendSMSVCode_BindMobile();
                } else {
                    $("#bindMobile_errorInfo").text(res.ErrorInfo);
                    //刷新验证码图片
                    InitVCode("imgVerifyCode");
                }
            }
        });
    }

    var SMSVCodeNumber_BindMobile = 60;//秒数
    var SMSVCodeNumber_timer;
    //发送短信倒计时
    function btnSendSMSVCode_BindMobile() {
        if (SMSVCodeNumber_BindMobile == 0) {
            $("#btnSendSMSVCode_BindMobile").text("重新发送验证码").removeAttr("disabled");
            SMSVCodeNumber_BindMobile = 60;
            return;
        } else {
            $("#btnSendSMSVCode_BindMobile").text("重新获取(" + SMSVCodeNumber_BindMobile + ")").attr("disabled", "true");
            SMSVCodeNumber_BindMobile--;
        }

        SMSVCodeNumber_timer = setTimeout(function () { btnSendSMSVCode_BindMobile() }, 1000);
    }

    $("#txtBindMobilePassWord2").on("keypress", function () {
        if (event.keyCode == 13) {
            AJAX_BindMobile_Save();
        }
    });

    //绑定手机
    function AJAX_BindMobile_Save() {
        var mobile = $.trim($("#txtBindMobile").val());
        var password1 = $.trim($("#txtBindMobilePassWord1").val());
        var password2 = $.trim($("#txtBindMobilePassWord2").val());

        if (mobile == '') {
            $("#bindMobile_Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入手机号码！");
            return false;
        }

        if (mobile.search(/^[1][3,4,5,6,7,8,9][0-9]{9}$/g)) {
            $("#bindMobile_Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入正确的手机号码！");
            return false;
        }

        $("#bindMobile_Verify").text("√");
        $("#bindMobile_errorInfo").text("");

        if ($.trim($("#txtBindMobileVerifyCode").val()) == '') {
            $("#bindMobile_CodeVerify").text("×");
            $("#bindMobile_errorInfo").text("请输入图形验证码！");
            return false;
        }

        $("#bindMobile_CodeVerify").text("√");
        $("#bindMobile_errorInfo").text("");

        if ($.trim($("#txtBindMobileSMSVCode").val()) == '') {
            $("#bindMobile_SMSVCodeVerify").text("×");
            $("#bindMobile_errorInfo").text("请输入短信验证码！");
            return false;
        }

        $("#bindMobile_SMSVCodeVerify").text("√");
        $("#bindMobile_errorInfo").text("");

        if (password1 == '') {
            $("#bindMobile_PassWord1Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入新密码！");
            return false;
        }

        if (password1.search(/^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,}$/g)) {
            $("#bindMobile_PassWord1Verify").text("×");
            $("#bindMobile_errorInfo").text("请设置6-20位数字字母组合的密码！");
            return false;
        }

        $("#bindMobile_PassWord1Verify").text("√");
        $("#bindMobile_errorInfo").text("");

        if (password2 == '') {
            $("#bindMobile_PassWord2Verify").text("×");
            $("#bindMobile_errorInfo").text("请输入确认密码！");
            return false;
        }

        if (password1 != password2) {
            $("#bindMobile_PassWord2Verify").text("×");
            $("#bindMobile_errorInfo").text("前后密码不一致！");
            return false;
        }

        $("#bindMobile_PassWord2Verify").text("√");
        $("#bindMobile_errorInfo").text("");

        $.ajax({
            url: "/Member/LoginForAccount/WXBind_Save",
            type: "post",
            data: $("#form_BindMobile").serializeArray(),
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    $("#bindMobile2").hide();
                    $("#bindMobile3").show();
                    $(".resettop dl").addClass("current_tab03");
                }
                else {
                    $("#bindMobile_errorInfo").text(res.ErrorInfo);
                    //刷新验证码图片
                    InitVCode("imgVerifyCode");
                }
            }
        });
    }

    //重新登录
    function BindMobileLogin() {
        $("#login_Tab").show();
        $("#loginMobile").show();
        $("#bindMobile").hide();
        //$("#bindMobile1").show();
        //$("#bindMobile3").show();
        $(".resettop dl").removeClass("current_tab02 current_tab03");
    }

    //====立即绑定手机结束

    //====忘记密码开始

    var updatePwdTimer, updatePwdTicket;

    //刷新二维码
    function RefreshUpdatePwdQrCode() {
        GetUpdatePwdQrCode();
    }

    //获取二维码
    function GetUpdatePwdQrCode() {
        $.ajax({
            url: "/Member/LoginForAccount/GetQRCode_ChangePassword",
            type: "get",
            ShowLoading: false,
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    if (res.Ticket == null) {
                        $("#updatePwd_qrcodeFailed").show();
                    }
                    else {
                        $("#updatePwd_loading").hide();
                        $("#updatePwd_qrcodeWX").show().attr("src", res.QrCode);
                        $("#updatePwd_qrcodeWX").attr("src", res.QrCode);
                        updatePwdTicket = res.Ticket;
                        $("#updatePwd_qrcodeFailed").hide();
                        IntervalUpdatePwdQrStatus();
                    }
                }
