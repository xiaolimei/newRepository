    //检查二维码状态
    function IntervalUpdatePwdQrStatus() {
        var num = 30;
        updatePwdTimer = setInterval(function CheckUpdatePwdQrStatus() {
            num--;

            $.ajax({
                url: "/Member/LoginForAccount/CheckQrStatus_ChangePassword?ticket=" + updatePwdTicket,
                type: "get",
                ShowLoading: false,
                dataType: "json",
                success: function (res) {
                    if (res.StatusCode == "Success") {
                        if (res.IsDisable == true) {
                            clearInterval(updatePwdTimer);
                            $("#updatePwd_qrcodeInfo").text("您的账号已被禁用！");
                            return;
                        } 

                        if (res.BindMobile == false) {
                            clearInterval(updatePwdTimer);
                            $("#updatePwd_qrcodeInfo").text("您尚未绑定手机号，请先进行绑定！");
                            //layer.alert("您尚未绑定手机号，请先进行绑定！", { skin: "KxLayer", closeBtn: 0 }, function (index) {
                            //    $("#bindMobile").show();
                            //    $("#updatePwd").hide();
                            //    GetBindMobileQrCode();
                            //    layer.close(index);
                            //});
                            return;
                        }

                        if (res.QrStatus == true) {
                            clearInterval(updatePwdTimer);
                            $("#updatePwd1").hide();
                            $("#updatePwd2").show();
                            $(".resettop dl").addClass("current_tab02");
                            $("#hddUpdatePwdOpenID").val(res.OD);
                            $("#updatePwd_customerMobile").text(res.mobile);
                            return;
