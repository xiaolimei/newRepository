    //忘记密码
    function AJAX_UpdatePwd_Save() {
        var password1 = $.trim($("#txtUpdatePwd1").val());
        var password2 = $.trim($("#txtUpdatePwd2").val());

        if (password1 == '') {
            $("#updatePwd1Verify").text("×");
            $("#updatePwd_errorInfo").text("请输入新密码！");
            return false;
        }

        if (password1.search(/^(?![0-9]+$)(?![a-zA-Z]+$)[0-9A-Za-z]{6,}$/g)) {
            $("#updatePwd1Verify").text("×");
            $("#updatePwd_errorInfo").text("请设置6-20位数字字母组合的密码！");
            return false;
        }

        $("#updatePwd1Verify").text("√");
        $("#updatePwd_errorInfo").text("");

        if (password2 == '') {
            $("#updatePwd2Verify").text("×");
            $("#updatePwd_errorInfo").text("请输入确认密码！");
            return false;
        }

        if (password1 != password2) {
            $("#updatePwd2Verify").text("×");
            $("#updatePwd_errorInfo").text("前后密码不一致！");
            return false;
        }

        $("#updatePwd2Verify").text("√");
        $("#updatePwd_errorInfo").text("");

        $.ajax({
            url: "/Member/LoginForAccount/ChangePassword_Save",
            type: "post",
            data: $("#form_UpdatePwd").serializeArray(),
            dataType: "json",
            success: function (res) {
                if (res.StatusCode == "Success") {
                    $("#updatePwd2").hide();
                    $("#updatePwd3").show();
                    $(".resettop dl").addClass("current_tab03");
                }
                else {
                    $("#updatePwd_errorInfo").text(res.ErrorInfo);
                }
            }
        });
    }

    //重新登录
    function UpdatePwdLogin() {
        $("#login_Tab").show();
        $("#loginMobile").show();
        $("#updatePwd").hide();
        //$("#updatePwd1").show();
        //$("#updatePwd3").show();
        $(".resettop dl").removeClass("current_tab02 current_tab03");

    }

    //====忘记密码结束
</script>
